  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Console List directory",
      "cargo": {
        "args": [
          "build",
          "--bin",
          "console"
        ]
      },
      "args": [
        "list-directory",
        "pc-ulrich.eden.lan",
        "/home"
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "WS Sync",
      "cargo": {
        "args": [
          "build",
          "--bin",
          "ws_sync"
        ]
      },
      "args": [
        "pc-ulrich.eden.lan",
        "192.168.101.14"
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Client",
      "cargo": {
        "args": [
          "build",
          "--bin",
          "client"
        ]
      }
    }
  ]
}