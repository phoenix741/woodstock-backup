    "@vue/tsconfig": "^0.1.3",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.6.2",
    "sass": "^1.52.0",
    "typescript": "~4.6.4",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.15"
