    "graphql": "^16.5.0",
    "numeral": "^2.0.6",
    "villus": "^1.1.0",
    "vue": "^3.2.34",
    "vue-echarts": "^6.0.3",
    "vue-router": "^4.0.15",
    "vuetify": "^3.0.0-beta.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
