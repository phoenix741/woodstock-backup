{
  "name": "@woodstock/core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "protoc": "protoc --plugin=./node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./libs/shared/src/shared ./woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false,onlyTypes=true --ts_proto_opt=env=node --ts_proto_opt=fileSuffix=.interface --experimental_allow_proto3_optional",
    "protoc:grpcjs": "protoc --plugin=./node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./libs/shared/src/shared ./woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false,outputServices=grpc-js --ts_proto_opt=env=node --ts_proto_opt=fileSuffix=-client.interface --experimental_allow_proto3_optional",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:api": "nest build -- api",
    "build:console": "nest build -- console",
    "build:client": "nest build -- client",
    "build:schedule": "nest build -- scheduleWorker",
    "build:stats": "nest build -- statsWorker",
    "build:backup": "nest build -- backupWorker",
    "build:refcnt": "nest build -- refcntWorker",
    "buildall": "concurrently --timings -n w: npm:build:*",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:api": "nest start -- api",
    "start:console": "nest start -- console",
    "start:client": "nest start -- client",
    "start:schedule": "nest start -- scheduleWorker",
    "start:stats": "nest start -- statsWorker",
    "start:backup": "nest start -- backupWorker",
    "start:refcnt": "nest start -- refcntWorker",
    "startall": "concurrently -n w: 'npm:start:*(!dev|prod|debug)'",
    "start:dev": "nest start --watch",
    "start:dev:api": "nest start --watch -- api",
    "start:dev:console": "nest start --watch -- console",
    "start:dev:client": "nest start --watch -- client",
    "start:dev:schedule": "nest start --watch -- scheduleWorker",
    "start:dev:stats": "nest start --watch -- statsWorker",
    "start:dev:backup": "nest start --watch -- backupWorker",
    "start:dev:refcnt": "nest start --watch -- refcntWorker",
    "startdevall": "concurrently -n w: 'npm:start:dev:*'",
    "start:debug": "nest start --debug --watch",
    "start:prod": "concurrently -n w: 'npm:start:prod:*'",
    "start:prod:api": "node dist/apps/api/main",
    "start:prod:console": "node dist/apps/console/main",
    "start:prod:client": "node dist/apps/client/main",
    "start:prod:schedule": "node dist/apps/scheduleWorker/main",
    "start:prod:stats": "node dist/apps/statsWorker/main",
    "start:prod:backup": "node dist/apps/backupWorker/main",
    "start:prod:refcnt": "node dist/apps/refcntWorker/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/servers/test/jest-e2e.json",
    "console:debug": "node --inspect-brk -r ts-node/register apps/console/src/main.ts",
    "console:dev": "ts-node -r tsconfig-paths/register apps/console/src/main.ts",
    "console": "node dist/apps/console/console.js",
    "predeb": "npm run build && cd ../client/ && npm run build",
    "deb": "npm run predeb && node-deb -- ./dist/main.js ./dist/ ./config/ ../client/dist/"
  },
  "node_deb": {
    "init": "systemd",
    "dependencies": "btrfs-compsize, btrfs-progs, coreutils, samba-common-bin, rsync, redis-server",
    "templates": {
      "default_variables": "./config/node-deb/default.template",
      "systemd_service": "./config/node-deb/systemd.service.template",
      "executable": "./config/node-deb/executable.template"
    },
    "entrypoints": {
      "daemon": "./dist/main.js",
      "cli": "./dist/console.js"
    },
    "user": "root"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.14",
    "@grpc/proto-loader": "^0.7.6",
    "@nestjs/apollo": "^10.2.1",
    "@nestjs/bullmq": "^1.1.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/graphql": "^10.2.1",
    "@nestjs/microservices": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@types/node-forge": "^1.3.2",
    "apollo-server-core": "~3.10.0",
    "apollo-server-express": "~3.10.0",
    "archiver": "^5.3.1",
    "bullmq": "^3.12.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^10.0.1",
    "cron-parser": "^4.8.1",
    "express": "^4",
    "file-type": "^18.3.0",
    "fs-extra": "^11.1.1",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.3.20",
    "ioredis": "^5.3.2",
    "ix": "5.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "logform": "^2.5.1",
    "minimatch": "^5.1.6",
    "mkdirp": "^2.1.6",
    "multistream": "^4.1.0",
    "nestjs-console": "^8.0.0",
    "node-forge": "^1.3.1",
    "prom-client": "^14.2.0",
    "prometheus-query": "^3.3.2",
    "protobufjs": "^7.2.3",
    "redlock": "^5.0.0-beta2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rsync": "^0.6.1",
    "rxjs": "^7.8.1",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.6.2",
    "tmp": "^0.2.1",
    "uuid": "^9.0.0",
    "webpack": "^5",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/archiver": "^5.3.2",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/minimatch": "^3.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/mock-fs": "^4.13.1",
    "@types/multistream": "^4.1.0",
    "@types/node": "^18.16.2",
    "@types/proper-lockfile": "^4.1.2",
    "@types/rsync": "^0.4.32",
    "@types/shelljs": "^0.8.12",
    "@types/supertest": "^2.0.12",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "mock-fs": "^5.2.0",
    "node-deb": "^0.10.7",
    "node-mocks-http": "^1.12.2",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.146.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^@woodstock/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "(.+)\\.js": "$1",
      "^@woodstock/workflow(|/.*)$": "<rootDir>/libs/workflow/src/$1"
    }
  }
}
