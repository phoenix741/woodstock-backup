{
  "name": "@woodstock/core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "protoc": "protoc --plugin=./node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./libs/shared/src/models ./woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false --ts_proto_opt=env=node --experimental_allow_proto3_optional",
    "protoc:grpcjs": "protoc --plugin=./node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./libs/shared/src/models ./libs/shared/woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false,outputServices=grpc-js --ts_proto_opt=env=node --experimental_allow_proto3_optional",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:all": "concurrently -n api,console,backup,schedule,stats,client \"nest build api\" \"nest build console\" \"nest build backupWorker\" \"nest build scheduleWorker\" \"nest build statsWorker\" \"nest build client\"",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:all": "concurrently -n api,backup,schedule,stats,client \"nest start api\" \"nest start backupWorker\" \"nest start scheduleWorker\" \"nest start statsWorker\" \"nest start client\"",
    "start:dev": "nest start --watch",
    "start:dev:all": "concurrently -n api,backup,schedule,stats,client \"nest start api --watch\" \"nest start backupWorker --watch\" \"nest start scheduleWorker --watch\" \"nest start statsWorker --watch\" \"nest start client --watch\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/servers/test/jest-e2e.json",
    "console:debug": "node --inspect-brk -r ts-node/register apps/console/src/main.ts",
    "console:dev": "ts-node -r tsconfig-paths/register apps/console/src/main.ts",
    "console": "node dist/apps/console/console.js",
    "predeb": "npm run build && cd ../client/ && npm run build",
    "deb": "npm run predeb && node-deb -- ./dist/main.js ./dist/ ./config/ ../client/dist/"
  },
  "node_deb": {
    "init": "systemd",
    "dependencies": "btrfs-compsize, btrfs-progs, coreutils, samba-common-bin, rsync, redis-server",
    "templates": {
      "default_variables": "./config/node-deb/default.template",
      "systemd_service": "./config/node-deb/systemd.service.template",
      "executable": "./config/node-deb/executable.template"
    },
    "entrypoints": {
      "daemon": "./dist/main.js",
      "cli": "./dist/console.js"
    },
    "user": "root"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.12",
    "@nestjs/bullmq": "^0.1.0",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.4.5",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/microservices": "^8.4.5",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@types/node-forge": "^1.0.2",
    "apollo-server-core": "~3.8.1",
    "apollo-server-express": "~3.8.1",
    "apollo-type-bigint": "^0.1.3",
    "archiver": "^5.3.1",
    "bullmq": "^1.86.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "commander": "^8.3.0",
    "cron-parser": "^4.4.0",
    "express": "^4",
    "file-type": "^17.1.1",
    "fs-extra": "^10.1.0",
    "graphql": "^15.8.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.2.11",
    "ix": "4.5.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "logform": "^2.4.0",
    "minimatch": "^5.1.0",
    "mkdirp": "^1.0.4",
    "multistream": "^4.1.0",
    "nestjs-console": "^7.0.1",
    "node-forge": "^1.3.1",
    "ora": "^5.4.1",
    "prom-client": "^14.0.1",
    "prometheus-query": "^3.2.3",
    "proper-lockfile": "^4.1.2",
    "protobufjs": "^6.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "rxjs": "^7.5.5",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.4.0",
    "tmp": "^0.2.1",
    "uuid": "^8.3.2",
    "webpack": "^5",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/archiver": "^5.3.1",
    "@types/bull": "^3.15.8",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/minimatch": "^3.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/mock-fs": "^4.13.1",
    "@types/multistream": "^4.1.0",
    "@types/node": "^16.11.36",
    "@types/proper-lockfile": "^4.1.2",
    "@types/rsync": "^0.4.32",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.12",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "concurrently": "^7.2.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "mock-fs": "^5.1.2",
    "node-deb": "^0.10.7",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "ts-proto": "^1.112.2",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@woodstock/shared(|/.*)$": "<rootDir>/libs/shared/src/$1"
    }
  }
}
