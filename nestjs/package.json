{
  "name": "@woodstock/core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "protoc": "protoc --plugin=./node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./libs/shared/src/protobuf ./woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false,onlyTypes=true         --ts_proto_opt=env=node --ts_proto_opt=fileSuffix=.interface --experimental_allow_proto3_optional",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:api": "nest build -- api",
    "build:console": "nest build -- console",
    "build:client": "nest build -- client",
    "build:schedule": "nest build -- scheduleWorker",
    "build:stats": "nest build -- statsWorker",
    "build:backup": "nest build -- backupWorker",
    "build:refcnt": "nest build -- refcntWorker",
    "buildall": "concurrently --timings -n w: npm:build:*",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:api": "nest start -- api",
    "start:console": "nest start -- console",
    "start:client": "nest start -- client",
    "start:schedule": "nest start -- scheduleWorker",
    "start:stats": "nest start -- statsWorker",
    "start:backup": "nest start -- backupWorker",
    "start:refcnt": "nest start -- refcntWorker",
    "startall": "concurrently -n w: 'npm:start:*(!dev|prod|debug)'",
    "start:dev": "nest start --watch",
    "start:dev:api": "nest start --watch -- api",
    "start:dev:console": "nest start --watch -- console",
    "start:dev:client": "nest start --watch -- client",
    "start:dev:schedule": "nest start --watch -- scheduleWorker",
    "start:dev:stats": "nest start --watch -- statsWorker",
    "start:dev:backup": "nest start --watch -- backupWorker",
    "start:dev:refcnt": "nest start --watch -- refcntWorker",
    "startdevall": "concurrently -n w: 'npm:start:dev:*'",
    "start:debug": "nest start --debug --watch",
    "start:prod": "concurrently -n w: 'npm:start:prod:*'",
    "start:prod:api": "node dist/apps/api/main",
    "start:prod:console": "node dist/apps/console/main",
    "start:prod:client": "node dist/apps/client/main",
    "start:prod:schedule": "node dist/apps/scheduleWorker/main",
    "start:prod:stats": "node dist/apps/statsWorker/main",
    "start:prod:backup": "node dist/apps/backupWorker/main",
    "start:prod:refcnt": "node dist/apps/refcntWorker/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/servers/test/jest-e2e.json",
    "console:debug": "node --inspect-brk -r ts-node/register apps/console/src/main.ts",
    "console:dev": "ts-node -r tsconfig-paths/register apps/console/src/main.ts",
    "console": "node dist/apps/console/console.js"
  },
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "@grpc/grpc-js": "^1.10.6",
    "@grpc/proto-loader": "^0.7.12",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.3.7",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.7",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.7",
    "@nestjs/platform-express": "^10.3.7",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@types/node-forge": "^1.3.11",
    "apollo-server-core": "~3.13.0",
    "archiver": "^7.0.1",
    "bullmq": "^5.7.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commander": "^12.0.0",
    "cron-parser": "^4.9.0",
    "express": "^4",
    "file-type": "^19.0.0",
    "fs-extra": "^11.2.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.1",
    "ioredis": "^5.3.2",
    "ix": "5.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "logform": "^2.6.0",
    "minimatch": "^9.0.4",
    "multistream": "^4.1.0",
    "nestjs-console": "^9.0.0",
    "node-forge": "^1.3.1",
    "ora": "^5.0.0",
    "prom-client": "^15.1.1",
    "prometheus-query": "^3.4.0",
    "protobufjs": "^7.2.6",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rsync": "^0.6.1",
    "rxjs": "^7.8.1",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.0",
    "tmp": "^0.2.3",
    "uuid": "^9.0.1",
    "webpack": "^5",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/archiver": "^6.0.2",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/minimatch": "^5.1.2",
    "@types/mock-fs": "^4.13.4",
    "@types/multistream": "^4.1.3",
    "@types/node": "^20.12.7",
    "@types/proper-lockfile": "^4.1.4",
    "@types/rsync": "^0.4.36",
    "@types/shelljs": "^0.8.15",
    "@types/supertest": "^6.0.2",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "node-mocks-http": "^1.14.1",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-proto": "^1.171.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^@woodstock/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "(.+)\\.js": "$1",
      "^@woodstock/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@woodstock/server(|/.*)$": "<rootDir>/libs/server/src/$1"
    }
  }
}
