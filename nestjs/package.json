{
  "name": "@woodstock/shared",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:api": "nest build -- api",
    "build:schedule": "nest build -- scheduleWorker",
    "build:stats": "nest build -- statsWorker",
    "build:backup": "nest build -- backupWorker",
    "build:refcnt": "nest build -- refcntWorker",
    "buildall": "concurrently --timings -n w: npm:build:*",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:api": "nest start -- api",
    "start:schedule": "nest start -- scheduleWorker",
    "start:stats": "nest start -- statsWorker",
    "start:backup": "nest start -- backupWorker",
    "start:refcnt": "nest start -- refcntWorker",
    "startall": "concurrently -n w: 'npm:start:*(!dev|prod|debug)'",
    "start:dev": "nest start --watch",
    "start:dev:api": "nest start --watch -- api",
    "start:dev:schedule": "nest start --watch -- scheduleWorker",
    "start:dev:stats": "nest start --watch -- statsWorker",
    "start:dev:backup": "nest start --watch -- backupWorker",
    "start:dev:refcnt": "nest start --watch -- refcntWorker",
    "startdevall": "concurrently -n w: 'npm:start:dev:*'",
    "start:debug": "nest start --debug --watch",
    "start:prod": "concurrently -n w: 'npm:start:prod:*'",
    "start:prod:api": "node dist/apps/api/main",
    "start:prod:schedule": "node dist/apps/scheduleWorker/main",
    "start:prod:stats": "node dist/apps/statsWorker/main",
    "start:prod:backup": "node dist/apps/backupWorker/main",
    "start:prod:refcnt": "node dist/apps/refcntWorker/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/servers/test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.0",
    "@woodstock/shared-rs": "file:../shared-rs",
    "apollo-server-core": "~3.13.0",
    "archiver": "^7.0.1",
    "bullmq": "^5.8.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron-parser": "^4.9.0",
    "express": "^4",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.1",
    "ioredis": "^5.4.1",
    "ix": "^5.0.0",
    "js-yaml": "^4.1.0",
    "logform": "^2.6.0",
    "node-forge": "^1.3.1",
    "prom-client": "^15.1.3",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.1",
    "tmp": "^0.2.3",
    "webpack": "^5",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.1",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.10",
    "@types/archiver": "^6.0.2",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.9",
    "@types/node-forge": "^1.3.11",
    "@types/shelljs": "^0.8.15",
    "@types/tmp": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.15.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^@woodstock/shared(|/.*)$": "<rootDir>/libs/server/src/$1",
      "(.+)\\.js": "$1"
    }
  }
}
