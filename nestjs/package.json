{
  "name": "@woodstock/core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "protoc": "protoc --plugin=./node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./libs/shared/src/models ./woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false --ts_proto_opt=env=node --experimental_allow_proto3_optional",
    "protoc:grpcjs": "protoc --plugin=./node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./libs/shared/src/models ./libs/shared/woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false,outputServices=grpc-js --ts_proto_opt=env=node --experimental_allow_proto3_optional",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:api": "nest build -- api",
    "build:console": "nest build -- console",
    "build:client": "nest build -- client",
    "build:schedule": "nest build -- scheduleWorker",
    "build:stats": "nest build -- statsWorker",
    "build:backup": "nest build -- backupWorker",
    "build:refcnt": "nest build -- refcntWorker",
    "buildall": "concurrently --timings -n w: npm:build:*",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:api": "nest start -- api",
    "start:console": "nest start -- console",
    "start:client": "nest start -- client",
    "start:schedule": "nest start -- scheduleWorker",
    "start:stats": "nest start -- statsWorker",
    "start:backup": "nest start -- backupWorker",
    "start:refcnt": "nest start -- refcntWorker",
    "startall": "concurrently -n w: 'npm:start:*(!dev|prod|debug)'",
    "start:dev": "nest start --watch",
    "start:dev:api": "nest start --watch -- api",
    "start:dev:console": "nest start --watch -- console",
    "start:dev:client": "nest start --watch -- client",
    "start:dev:schedule": "nest start --watch -- scheduleWorker",
    "start:dev:stats": "nest start --watch -- statsWorker",
    "start:dev:backup": "nest start --watch -- backupWorker",
    "start:dev:refcnt": "nest start --watch -- refcntWorker",
    "startdevall": "concurrently -n w: 'npm:start:dev:*'",
    "start:debug": "nest start --debug --watch",
    "start:prod": "concurrently -n w: 'npm:start:prod:*'",
    "start:prod:api": "node dist/apps/api/main",
    "start:prod:console": "node dist/apps/console/main",
    "start:prod:client": "node dist/apps/client/main",
    "start:prod:schedule": "node dist/apps/scheduleWorker/main",
    "start:prod:stats": "node dist/apps/statsWorker/main",
    "start:prod:backup": "node dist/apps/backupWorker/main",
    "start:prod:refcnt": "node dist/apps/refcntWorker/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/servers/test/jest-e2e.json",
    "console:debug": "node --inspect-brk -r ts-node/register apps/console/src/main.ts",
    "console:dev": "ts-node -r tsconfig-paths/register apps/console/src/main.ts",
    "console": "node dist/apps/console/console.js",
    "predeb": "npm run build && cd ../client/ && npm run build",
    "deb": "npm run predeb && node-deb -- ./dist/main.js ./dist/ ./config/ ../client/dist/"
  },
  "node_deb": {
    "init": "systemd",
    "dependencies": "btrfs-compsize, btrfs-progs, coreutils, samba-common-bin, rsync, redis-server",
    "templates": {
      "default_variables": "./config/node-deb/default.template",
      "systemd_service": "./config/node-deb/systemd.service.template",
      "executable": "./config/node-deb/executable.template"
    },
    "entrypoints": {
      "daemon": "./dist/main.js",
      "cli": "./dist/console.js"
    },
    "user": "root"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.8",
    "@grpc/proto-loader": "^0.7.0",
    "@nestjs/apollo": "^10.0.19",
    "@nestjs/bullmq": "^0.1.0",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/graphql": "^10.0.21",
    "@nestjs/microservices": "^9.0.8",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@types/node-forge": "^1.0.4",
    "apollo-server-core": "~3.10.0",
    "apollo-server-express": "~3.10.0",
    "archiver": "^5.3.1",
    "bullmq": "^1.87.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "commander": "^9.4.0",
    "cron-parser": "^4.6.0",
    "express": "^4",
    "file-type": "^17.1.6",
    "fs-extra": "^10.1.0",
    "graphql": "^16.5.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.3.1",
    "ix": "5.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "logform": "^2.4.2",
    "minimatch": "^5.1.0",
    "mkdirp": "^1.0.4",
    "multistream": "^4.1.0",
    "nestjs-console": "^8.0.0",
    "node-forge": "^1.3.1",
    "prom-client": "^14.0.1",
    "prometheus-query": "^3.2.5",
    "proper-lockfile": "^4.1.2",
    "protobufjs": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "rxjs": "^7.5.6",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.5.0",
    "tmp": "^0.2.1",
    "uuid": "^8.3.2",
    "webpack": "^5",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/archiver": "^5.3.1",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/minimatch": "^3.0.5",
    "@types/mkdirp": "^1.0.2",
    "@types/mock-fs": "^4.13.1",
    "@types/multistream": "^4.1.0",
    "@types/node": "^18.6.4",
    "@types/proper-lockfile": "^4.1.2",
    "@types/rsync": "^0.4.32",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.12",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "concurrently": "^7.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "mock-fs": "^5.1.4",
    "node-deb": "^0.10.7",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.121.2",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@woodstock/shared(|/.*)$": "<rootDir>/libs/shared/src/$1"
    }
  }
}
