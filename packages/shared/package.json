{
  "name": "@woodstock/shared",
  "version": "2.0.0",
  "description": "Code shared between the client and the server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "protoc": "protoc --plugin=../../node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./src/models ./woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false --ts_proto_opt=env=node --experimental_allow_proto3_optional",
    "protoc:grpcjs": "protoc --plugin=../../node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./src/models ./woodstock.proto -I. --ts_proto_opt=outputServices=none,outputEncodeMethods=false,outputJsonMethods=false,outputClientImpl=false,lowerCaseServiceMethods=false,forceLong=long,unrecognizedEnum=false,outputServices=grpc-js --ts_proto_opt=env=node --experimental_allow_proto3_optional",
    "build": "rimraf dist && tsc --build tsconfig.build.json",
    "build:watch": "tsc --build tsconfig.build.json --watch",
    "prepublish": "npm run build"
  },
  "author": "Ulrich Vandenhekke",
  "license": "MIT",
  "devDependencies": {
    "@grpc/grpc-js": "^1.4.5",
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/testing": "^8.2.4",
    "@types/jest": "^27.0.3",
    "@types/long": "^4.0.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.7",
    "jest": "^27.4.5",
    "long": "^4.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.5.1",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ix": "4.5.2",
    "rxjs": "^7.1.0",
    "ts-jest": "^27.1.2",
    "ts-proto": "^1.95.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@grpc/grpc-js": "^1.4.5",
    "@nestjs/common": "^8.2.4",
    "@nestjs/microservices": "^8.2.4",
    "@types/jest": "^27.0.3",
    "long": "^4.0.0",
    "mkdirp": "^1.0.4",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0",
    "ix": "4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
